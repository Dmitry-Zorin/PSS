{
	"name": "storage-system-server",
	"version": "2.1.0",
	"scripts": {
		"start": "nest start",
		"start:dev": "nest start -w",
		"prebuild": "rimraf dist",
		"build": "nest build",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
	},
	"dependencies": {
		"@nestjs/common": "^8.0.0",
		"@nestjs/config": "^1.0.1",
		"@nestjs/core": "^8.0.0",
		"@nestjs/jwt": "^8.0.0",
		"@nestjs/microservices": "^8.0.6",
		"@nestjs/mongoose": "^8.0.1",
		"@nestjs/passport": "^8.0.1",
		"@nestjs/platform-express": "^8.0.0",
		"@nestjs/typeorm": "^8.0.2",
		"amqp-connection-manager": "^3.2.3",
		"amqplib": "^0.8.0",
		"bcrypt": "^5.0.1",
		"class-transformer": "^0.4.0",
		"class-validator": "^0.13.1",
		"helmet": "^4.6.0",
		"joi": "^17.4.2",
		"lodash": "^4.17.21",
		"mongodb": "^4.1.1",
		"mongoose": "^5.13.7",
		"passport": "^0.4.1",
		"passport-jwt": "^4.0.0",
		"pg": "^8.7.1",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.2.0",
		"typeorm": "^0.2.37"
	},
	"devDependencies": {
		"@nestjs/cli": "^8.0.0",
		"@nestjs/schematics": "^8.0.0",
		"@nestjs/testing": "^8.0.6",
		"@types/bcrypt": "^5.0.0",
		"@types/express": "^4.17.13",
		"@types/jest": "^27.0.1",
		"@types/lodash": "^4.14.172",
		"@types/multer": "^1.4.7",
		"@types/node": "^16.0.0",
		"@types/node-fetch": "^2.5.12",
		"@types/passport-jwt": "^3.0.6",
		"@types/supertest": "^2.0.11",
		"jest": "27.0.6",
		"jest-extended": "^0.11.5",
		"node-fetch": "^2.6.1",
		"supertest": "^6.1.3",
		"ts-jest": "^27.0.3",
		"ts-loader": "^9.2.3",
		"ts-node": "^10.0.0",
		"tsconfig-paths": "^3.10.1",
		"typescript": "^4.3.5"
	},
	"jest": {
		"rootDir": "src",
		"moduleFileExtensions": [
			"js",
			"ts"
		],
		"testRegex": "spec\\.ts$",
		"transform": {
			"\\.ts$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.ts"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node",
		"setupFilesAfterEnv": [
			"<rootDir>/jest-setup.ts"
		]
	}
}
