{
	"name": "@pss/server",
	"scripts": {
		"start": "nest start --watch",
		"debug": "nest start --debug --watch",
		"prebuild": "rimraf dist",
		"build": "nest build",
		"lint": "eslint src --ext .ts --fix",
		"test": "jest --runInBand",
		"test:watch": "jest --watch",
		"test:cov": "jest --coverage",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"typeorm": "ts-node -r dotenv/config ./node_modules/typeorm/cli",
		"migrations": "run-p migration:run:*",
		"migration:run:auth": "npm run typeorm migration:run -- -d src/microservices/auth/data-source",
		"migration:run:resources": "npm run typeorm migration:run -- -d src/microservices/resources/data-source",
		"migration:revert:auth": "npm run typeorm migration:revert -- -d src/microservices/auth/data-source",
		"migration:revert:resources": "npm run typeorm migration:revert -- -d src/microservices/resources/data-source",
		"migration:generate:auth": "npm run typeorm migration:generate src/microservices/auth/migrations/auth -- -d src/microservices/auth/data-source --pretty",
		"migration:generate:resources": "npm run typeorm migration:generate src/microservices/resources/migrations/resources -- -d src/microservices/resources/data-source --pretty",
		"build:vercel": "nest build --webpack --webpackPath webpack.vercel.js",
		"vercel-build": "run-s build:vercel migrations"
	},
	"dependencies": {
		"@fastify/cors": "^7.0.0",
		"@fastify/helmet": "^8.1.0",
		"@nestjs/axios": "^0.0.8",
		"@nestjs/common": "^8.4.2",
		"@nestjs/config": "^2.0.0",
		"@nestjs/core": "^8.4.2",
		"@nestjs/jwt": "^8.0.0",
		"@nestjs/mapped-types": "^1.0.1",
		"@nestjs/microservices": "^8.4.2",
		"@nestjs/mongoose": "^9.0.3",
		"@nestjs/passport": "^8.2.1",
		"@nestjs/platform-fastify": "^8.4.5",
		"@nestjs/typeorm": "^8.0.3",
		"@types/multer-gridfs-storage": "^4.0.4",
		"@types/pluralize": "^0.0.29",
		"ajv-formats": "^2.1.1",
		"amqp-connection-manager": "^4.1.1",
		"amqplib": "^0.8.0",
		"bcryptjs": "^2.4.3",
		"class-transformer": "^0.5.1",
		"class-validator": "^0.13.2",
		"fastify": "^3.29.0",
		"form-data": "^4.0.0",
		"joi": "^17.6.0",
		"lodash": "^4.17.21",
		"mime": "^3.0.0",
		"mongoose": "^6.2.7",
		"multer-gridfs-storage": "^5.0.2",
		"npm-run-all": "^4.1.5",
		"passport": "^0.5.2",
		"passport-jwt": "^4.0.0",
		"pg": "^8.7.3",
		"pluralize": "^8.0.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.5.5",
		"typeorm": "^0.3.1"
	},
	"devDependencies": {
		"@nestjs/cli": "^8.2.4",
		"@nestjs/schematics": "^8.0.8",
		"@nestjs/testing": "^8.4.2",
		"@types/bcryptjs": "^2.4.2",
		"@types/jest": "^27.4.1",
		"@types/lodash": "^4.14.180",
		"@types/mime": "^2.0.3",
		"@types/multer": "^1.4.7",
		"@types/node": "^17.0.22",
		"@types/node-fetch": "^2.6.1",
		"@types/passport-jwt": "^3.0.6",
		"@types/supertest": "^2.0.12",
		"@typescript-eslint/eslint-plugin": "^5.25.0",
		"@typescript-eslint/parser": "^5.25.0",
		"@vercel/node": "^1.15.3",
		"dotenv": "^16.0.1",
		"eslint": "^8.15.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"jest": "27.5.1",
		"jest-extended": "^2.0.0",
		"node-fetch": "^3.2.3",
		"node-loader": "^2.0.0",
		"prettier": "^2.6.2",
		"supertest": "^6.2.2",
		"ts-jest": "^27.1.3",
		"ts-loader": "^9.2.8",
		"ts-node": "^10.7.0",
		"tsconfig-paths": "^3.14.0",
		"typescript": "^4.6.2"
	},
	"jest": {
		"rootDir": "src",
		"moduleFileExtensions": [
			"ts"
		],
		"testRegex": "spec\\.ts$",
		"transform": {
			"\\.ts$": "ts-jest"
		},
		"collectCoverageFrom": [
			"**/*.ts"
		],
		"coverageDirectory": "../coverage",
		"testEnvironment": "node",
		"setupFilesAfterEnv": [
			"<rootDir>/jest-setup.ts"
		]
	}
}
