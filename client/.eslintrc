{
	"root": true,
	"env": {
		"browser": true,
		"es2022": true
	},
	"parserOptions": {
		"sourceType": "module",
		"ecmaVersion": "latest"
	},
	"settings": {
		"react": {
			"version": "17"
		}
	},
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended",
		"plugin:react/jsx-runtime",
		"plugin:prettier/recommended"
	],
	"rules": {
		"react/prop-types": "off",
		"react/jsx-key": "off",
		"react/display-name": "off",
		"react/no-children-prop": "off"
	}
}
