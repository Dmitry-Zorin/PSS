{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"baseUrl": "src",
		"jsx": "react-jsx",
		"types": ["type-fest"],
		"noEmit": true,
		"strict": true,
		"incremental": true,
		"skipLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"strictPropertyInitialization": false
	},
	"include": ["src"]
}
